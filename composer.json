{
    "name": "zakiya/eight-starter",
    "description": "Template and Install profile for D8",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {

            "type": "package",
            "package": {
                "name": "zakiya/courts-theme",
                "version": "master",
                "type": "drupal-theme",
                "dist": {
                    "url": "https://github.com/zakiya/courts-theme/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "5.5.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/enyo/dropzone",
                    "type": "git",
                    "reference": "v5.5.0"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.5",
        "drupal/autosave_form": "^1.0",
        "drupal/chosen": "^2.8",
        "drupal/components": "^1.1",
        "drupal/config_exclude": "1.x-dev",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.9",
        "drupal/core": "^8.7.0",
        "drupal/default_content": "^1.0@alpha",
        "drupal/dropzonejs": "^2.0@alpha",
        "drupal/entity_browser": "^2.1",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entity_embed": "^1.0@RC",
        "drupal/environment_indicator": "^3.6",
        "drupal/extlink": "^1.1",
        "drupal/focal_point": "^1.0",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.29",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/linkit": "^4.3",
        "drupal/login_emailusername": "^1.1",
        "drupal/maxlength": "^1.0@beta",
        "drupal/media_entity_browser": "^2.0",
        "drupal/metatag": "^1.8",
        "drupal/module_filter": "^3.1",
        "drupal/paragraphs": "^1.9",
        "drupal/pathauto": "^1.4",
        "drupal/r4032login": "^1.1",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/radix": "^4.5",
        "drupal/redirect": "^1.3",
        "drupal/search404": "^1.0",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/search_api_solr": "^3.4",
        "drupal/search_api_spellcheck": "3.x-dev",
        "drupal/simple_block": "^1.0@beta",
        "drupal/smart_trim": "^1.1",
        "drupal/webform": "^5.4",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^9.0.0",
        "enyo/dropzone": "^5.5",
        "oomphinc/composer-installers-extender": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zakiya/courts-theme": "dev-master",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::postInstall",
            "find ./vendor/ -type d -name .git | xargs rm -rf",
            "find ./web/ -type d -name .git | xargs rm -rf"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::postInstall",
            "find ./vendor/ -type d -name .git | xargs rm -rf",
            "find ./web/ -type d -name .git | xargs rm -rf"
        ],
        "build-assets": [
            "@prepare-for-pantheon",
            "composer install --optimize-autoloader --no-dev"
        ],
        "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": "echo 'No code sniff step defined.'",
        "unit-test": "echo 'No unit test step defined.'",
        "artifact-sync": [
            "rsync -KPar --delete --force --files-from=scripts/deploy.txt ./ data/artifact/"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": [
            "library"
        ],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
